<h2>View Student/Index</h2>

<h2>Danh sách sinh viên</h2>
<table class="table border">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Age</th>
    </tr>
    @foreach (var item in ViewData["Student"] as List<Student>)
    {
        <tr>
            <td>@item.ID</td>
            <td>@item.Name</td>
            <td>@item.Age</td>
        </tr>

    }
</table>